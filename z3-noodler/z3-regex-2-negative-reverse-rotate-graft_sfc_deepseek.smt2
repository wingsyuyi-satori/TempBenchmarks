(declare-const S String)
(assert (str.in_re S (re.++ re.allchar (str.to_re "bbbaaa"))))
(assert (not (str.in_re S (re.++ re.allchar (str.to_re "bbb")))))

; --- LLM Generated Split Constraints ---
(declare-const first_part String)
(declare-const second_part String)
; Split on first occurrence of "a"
(assert (= first_part (str.split_at S "a" 0)))
(assert (= second_part (str.split_at S "a" 1)))
; Verify split produces non-empty parts
(assert (> (str.len first_part) 0))
(assert (> (str.len second_part) 0))
; ; Check number of splits when using "b" as delimiter
(assert (>= (str.num_splits S "b") 2))
; Get remaining segments after first split
(declare-const rest_parts String)
(assert (= rest_parts (str.split_rest S "a" 1)))
; --- End LLM Generated Constraints ---

(check-sat)

(get-model)
