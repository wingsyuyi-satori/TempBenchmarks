(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "[[")) (str.to_re "zz")))))
(assert (= (str.len x) 6))
(assert (not (= x "[[zzzz")))
(assert (not (= x "zz[[zz")))
(assert (not (= x "[[[[zz")))

; --- LLM Generated Split Constraints ---
(declare-const x_seg0 String)
(declare-const x_seg1 String)
(declare-const x_num_splits Int)
(assert (= x_seg0 (str.split_at x "z" 0)))
(assert (= x_seg1 (str.split_at x "z" 1)))
(assert (= x_num_splits (str.num_splits x "z")))
(assert (>= x_num_splits 2)) ; at least two segments if "z" occurs
(assert (not (= x_seg0 ""))) ; first segment is non-empty if x does not start with "z"
(declare-const x_rest String)
(assert (= x_rest (str.split_rest x "z" 1)))
; --- End LLM Generated Constraints ---

(check-sat)

(get-model)
