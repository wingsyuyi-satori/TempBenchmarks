(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (re.* (str.to_re "$$")) (str.to_re "}}")))))
(assert (str.in_re y (re.* (re.++ (re.* (str.to_re "$$")) (str.to_re "}}")))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 14))

; --- LLM Generated Split Constraints ---
(declare-const x_seg0 String)
(declare-const x_seg1 String)
(declare-const y_seg0 String)
(declare-const y_seg1 String)
; Split x and y at first occurrence of "$$"
(assert (= x_seg0 (str.split_at x "$$" 0)))
(assert (= x_seg1 (str.split_at x "$$" 1)))
(assert (= y_seg0 (str.split_at y "$$" 0)))
(assert (= y_seg1 (str.split_at y "$$" 1)))
; Number of splits for x and y with "$$" as delimiter
(assert (>= (str.num_splits x "$$") 1))
(assert (>= (str.num_splits y "$$") 1))
; The segments should not be empty
(assert (>= (str.len x_seg0) 0))
(assert (>= (str.len x_seg1) 0))
(assert (>= (str.len y_seg0) 0))
(assert (>= (str.len y_seg1) 0))
; --- End LLM Generated Constraints ---

(check-sat)

(get-model)
