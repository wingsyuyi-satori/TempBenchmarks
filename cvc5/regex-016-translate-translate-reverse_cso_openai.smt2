(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.union (str.to_re "YX~M") (str.to_re "321")))))
(assert (= 11 (str.len x)))
(assert (not (= x "YX~M321YX~M")))
(assert (not (= x "321YX~MYX~M")))

; --- LLM Generated Split Constraints ---
; Split x into segments by delimiter "YX~M"
(declare-const seg_cnt Int)
(assert (= seg_cnt (str.num_splits x "YX~M")))
(assert (>= seg_cnt 2))
; ; Extract the first and last segments after split by "YX~M"
(declare-const seg_first String)
(declare-const seg_last String)
(assert (= seg_first (str.split_at x "YX~M" 0)))
(assert (= seg_last (str.split_at x "YX~M" (- seg_cnt 1))))
; Nested: Split seg_last by "321"
(declare-const seg_last_0 String)
(declare-const seg_last_1 String)
(assert (= seg_last_0 (str.split_at seg_last "321" 0)))
(assert (= seg_last_1 (str.split_rest seg_last "321" 1)))
; Cross-dependency: seg_first must equal y
(assert (= seg_first y))
; Advanced validation:
; The sum of lengths of seg_last_0 and seg_last_1 must be at least 3
(assert (>= (+ (str.len seg_last_0) (str.len seg_last_1)) 3))
; seg_last_1 must not be empty
(assert (> (str.len seg_last_1) 0))
; seg_first and seg_last_0 must not be equal
(assert (not (= seg_first seg_last_0)))
; --- End LLM Generated Constraints ---

(check-sat)

(get-model)
