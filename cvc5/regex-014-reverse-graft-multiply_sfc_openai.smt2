(assert (= x_seg1 (str.split_at x "b" 1)))
; There should be two splits for x with delimiter "b"
(assert (= (str.num_splits x "b") 2))
; Split y at "aa" and access first two segments
(declare-const y_seg0 String)
(declare-const y_seg1 String)
(assert (= y_seg0 (str.split_at y "aa" 0)))
(assert (= y_seg1 (str.split_at y "aa" 1)))
; There should be three splits for y with delimiter "aa"
(assert (= (str.num_splits y "aa") 3))
; Validation: lengths of segments are non-negative
(assert (>= (str.len x_seg0) 0))
(assert (>= (str.len x_seg1) 0))
(assert (>= (str.len y_seg0) 0))
(assert (>= (str.len y_seg1) 0))
